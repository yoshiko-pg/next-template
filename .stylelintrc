{
  "extends": [
    "stylelint-config-standard",
    "stylelint-config-recess-order"
  ],
    "plugins": [
      "stylelint-order"
    ],
    "rules": {
      "no-descending-specificity": null,
      "rule-empty-line-before": null,
      "order/order": [
        "custom-properties",
        "declarations"
      ],
    }
}